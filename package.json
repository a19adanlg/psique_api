{
  "name": "psique_api",
  "version": "1.0.0",
  "description": "Psique_API: Proyecto de fin de ciclo DAW - A19AdanLG",
  "main": "./src/app.js",
  "scripts": {
    "seed": "node seed.db/seed_db.js",
    "prestart": "npm run dev:mongo-stop && npm run dev:mongo-start",
    "start": "nodemon ./src/app.js",
    "predebug": "npm run dev:mongo-stop && npm run dev:mongo-start",
    "debug": "nodemon --inspect ./src/app.js",
    "postdebug": "npm run dev:mongo-stop",
    "dev:mongo-start": "docker run --name psique_db -d -p 27017:27017 -v psique_db:/data/db mongo:4.0",
    "dev:mongo-stop": "docker rm -f psique_db",
    "dev:mongo-enter": "docker exec -ti psique_db mongo"
  },
  "keywords": [],
  "author": "Adán Lijó González",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-logger-unique-req-id": "^1.0.4",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
